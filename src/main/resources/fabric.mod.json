{
  "schemaVersion": 1,
  "id": "squake",
  "version": "${version}",

  "name": "Squake",
  "description": "A Quake-style movement mod for Minecraft. Originally made by squeek502, ported to newer versions by Zeitheron, ported to fabric by JavaJumper",
  "authors": [
    "Zeitheron",
    "JavaJumper"
  ],
  "contact": {
    "homepage": "https://javajump3r.github.io/",
    "sources": "https://github.com/JavaJump3r/"
  },
  "accessWidener" : "squake.accesswidener",
  "license": "GPL v3",

  "environment": "*",
  "entrypoints": {
    "client": [
      "org.zeith.comm12.squake.Squake"
    ],
    "main": [
      "org.zeith.comm12.squake.Squake"
    ],
    "modmenu": [
      "org.zeith.comm12.squake.ModMenuIntegration"
    ]
  },
  "mixins": [
    "squake.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.19.3",
    "java": ">=17",
    "yet-another-config-lib": ">=2.2.0",
    "jjautoconfig": "1.0.1"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/44sD49Aan3"
      }
    }
  }
}
